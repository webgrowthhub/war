    $(document).ready(function(){
  

    $("#resetformOn").on("click",function(e){
       var password= $("#password").val();
    var cpassword= $("#cpassword").val();
  

    $(".password,.cpassword,.pcpassword").hide();
    if(password == '' || password == undefined){
      $(".password").show();
      
      event.preventDefault();
    }else if(cpassword == '' || cpassword == undefined){
     $(".cpassword").show();
     event.preventDefault();
    }else if(password != cpassword){
      $(".pcpassword").show();
      event.preventDefault();
    }
    })
      
  
   
  


      $( "#submit_reg" ).on("click",function( event ) {
    
    var firstname= $("#firstname").val();
    var lastname= $("#lastname").val();
    var userid= $("#userid").val();
    
    var email_id= $("#email_id").val();
    var cemail= $("#cemail").val();
    var mobile= $("#mobile").val();
    var ckbox = $('#term_condition');
    if (ckbox.is(':checked')) {
           var checkbox='1';
        } else {
          var checkbox='0';
        }

    $(".firstname,.lastname,.userid,.email_id,.cemail,.pcemail,.mobile,.allfields").hide();
    if(firstname == '' || firstname == undefined){
      $(".firstname").show();
      $(".allfields").show();
      
      event.preventDefault();
    }else if(lastname == '' || lastname == undefined){
     $(".lastname").show();
     $(".allfields").show();
     event.preventDefault();
    }else if(userid == '' || userid == undefined){
      $(".userid").show();
      $(".allfields").show();
      event.preventDefault();
    }else if(email_id == '' || email_id == undefined){
      $(".email_id").show();
      event.preventDefault();
    }else if(cemail == '' || cemail == undefined){
      $(".cemail").show();
      $(".allfields").show();
      event.preventDefault();
    }else if(email_id != cemail){
      $(".pcemail").show();
      $(".allfields").show();
      event.preventDefault();
    }else if(mobile == '' || mobile == undefined){
      $(".mobile").show();
      $(".allfields").show();
      event.preventDefault();
    }else if(checkbox == '0'){
      $(".term_condition").show();
      event.preventDefault();
    }else{
    
    }
    

  });
  


  
