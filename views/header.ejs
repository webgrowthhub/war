            <div class="collapse navbar-collapse col-6 col-lg-7 col-md-7 justify-content-end" id="collapsibleNavbar">
            <ul class="navbar-nav">
                <li class="nav-item">
                <a class="nav-link" href="/">Home</a>
                </li>
                <% if ( typeof(usersession) !='undefined') { %>

                  <li class="nav-item">
                    <a class="nav-link" href="http://emeraldparkmemorial.com.au/profile"> Profile </a>
                    </li>

                  <% } %>
                <li class="nav-item">
